{
  "name": "parcel-starter",
  "version": "1.0.1",
  "description": "Simple Web Starter for React, supports Javascript and Typescript",
  "main": "dist/main/index.js",
  "browser": "dist/browser/index.js",
  "scripts": {
    "start": "parcel serve src/index.html",
    "build": "parcel build src/index.html",
    "test": "jest --passWithNoTests",
    "clean": "rimraf dist .cache",
    "clean:dist": "rimraf node_modules",
    "clean:all": "yarn clean:dist && yarn clean:pkg",
    "lint": "eslint \"./src/**/*.{js,ts,tsx}\"",
    "lint:fix": "yarn lint --quiet --fix",
    "types": "tsc --noEmit -p .",
    "types:watch": "tsc --noEmit -p . --watch",
    "validate": "yarn types && yarn lint && yarn test"
  },
  "keywords": [
    "starter",
    "parcel",
    "react",
    "typescript"
  ],
  "author": "Pervez Iqbal",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.13.10",
    "@babel/plugin-proposal-class-properties": "7.13.0",
    "@parcel/transformer-typescript-tsc": "2.0.0-beta.2",
    "@parcel/validator-typescript": "2.0.0-beta.2",
    "@testing-library/jest-dom": "5.11.9",
    "@testing-library/react": "11.2.5",
    "@types/chance": "1.1.1",
    "@types/jest": "26.0.21",
    "@types/jsdom": "16.2.7",
    "@types/react": "17.0.3",
    "@types/react-dom": "17.0.3",
    "@types/react-router": "5.1.12",
    "@types/react-router-dom": "5.1.7",
    "@typescript-eslint/eslint-plugin": "4.19.0",
    "@typescript-eslint/parser": "4.19.0",
    "babel-eslint": "10.1.0",
    "chance": "1.1.7",
    "confusing-browser-globals": "1.0.10",
    "eslint": "7.22.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-functional": "3.2.1",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.3.2",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-no-null": "1.0.2",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.23.1",
    "eslint-plugin-unicorn": "29.0.0",
    "jest": "26.6.3",
    "jsdom": "16.5.1",
    "npm-check-updates": "11.3.0",
    "parcel": "2.0.0-beta.2",
    "postcss": "^8.2.8",
    "prettier": "2.2.1",
    "rimraf": "3.0.2",
    "ts-jest": "26.5.4",
    "typescript": "4.2.3"
  },
  "browserslist": [
    "defaults",
    "not IE 11",
    "maintained node versions"
  ],
  "dependencies": {
    "date-fns": "2.19.0",
    "formik": "2.2.6",
    "immer": "9.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "use-immer": "0.5.1",
    "yup": "0.32.9"
  }
}
